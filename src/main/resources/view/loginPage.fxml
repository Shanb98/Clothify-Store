<AnchorPane prefHeight="637.0" prefWidth="948.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.icet.controller.LoginController">
   <children>
      <ImageView fitHeight="645.0" fitWidth="958.0" layoutY="-1.0">
         <image>
            <Image url="@../images/pexels-ron-lach-9603489.jpg" />
         </image>
      </ImageView>
      <ImageView fitHeight="46.0" fitWidth="49.0" layoutX="18.0" layoutY="8.0" pickOnBounds="true" preserveRatio="true">
