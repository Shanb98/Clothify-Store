            <Image url="@../images/image-removebg-preview.png" />
         </image>
      </ImageView>
      <Label layoutX="70.0" layoutY="-8.0" text="Clothify Store">
         <font>
            <Font name="Papyrus" size="36.0" />
         </font>
      </Label>
      <ImageView fitHeight="211.0" fitWidth="286.0" layoutX="441.0" layoutY="56.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/robo.png" />
         </image>
      </ImageView>
      <Label layoutX="70.0" layoutY="104.0" text="WELCOME" textFill="WHITE">
         <font>
            <Font name="Courier New" size="60.0" />
         </font>
      </Label>
      <TextField fx:id="txtUsername" layoutX="71.0" layoutY="251.0" prefHeight="47.0" prefWidth="466.0" promptText="USERNAME" style="-fx-background-color: #E3E5DF;">
